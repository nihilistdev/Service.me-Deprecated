{
  "name": "service.me",
  "description": "Simple service provider app to maintain your work",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "compile": "tsc",
    "start:watch": "tsc -w",
    "start:dev": "nodemon dist/index.js",
    "gen:types-development": "npx gen-env-types config/.env.developmet -o src/types/env.d.ts",
    "gen:migrations": "typeorm migration:generate --config ./dist/database/config/ormconfig.js --dir ./src/database/migrations -n $1",
    "revert:migrations": "typeorm migration:revert",
    "run:migrations": "typeorm migration:run --config ./dist/database/config/ormconfig.js",
    "format": "prettier . --write",
    "setup:db": "ts-node ./src/utils/testing/setup.ts",
    "test": "npm run setup:db && jest --detectOpenHandles",
    "start:coverage": "npm run setup:db && node --experimental-vm-modules node_modules/.bin/jest --coverage"
  },
  "keywords": [
    "service"
  ],
  "_moduleAliases":{
    "src":"dist/"
  },
  "author": "Filip Ivanusec",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.28.5",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "ioredis": "^4.28.5",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.45",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.2.0",
    "@types/connect-redis": "^0.0.18",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^29.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.24",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "jest": "28.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "28.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
