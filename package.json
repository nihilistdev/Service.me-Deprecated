{
  "name": "service.me",
  "description": "Simple service provider app to maintain your work",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc",
    "start:watch": "tsc -w",
    "start:dev": "nodemon dist/index.js",
    "gen:types-development": "npx gen-env-types config/.env.developmet -o src/types/env.d.ts",
    "gen:migrations": "typeorm -d ./src/database/ormconfig.js migration:generate -n serviceme",
    "revert:migrations": "typeorm migration:revert",
    "run:migrations": "typeorm migration:run",
    "typeorm": "typeorm-ts-node-esm -d ./src/database/dbConfig.ts",
    "format": "prettier . --write",
    "test": "jest"
  },
  "keywords": [
    "service"
  ],
  "author": "Filip Ivanusec",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.28.5",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "ioredis": "^4.28.5",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^28.1.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.24",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "jest": "^28.1.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
